{
  "name": "@luxluth/onigiri",
  "description": "A simple video player for the web",
  "private": false,
  "keywords": [
    "video",
    "player",
    "web",
    "html",
    "javascript",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxluth/onigiri.git"
  },
  "author": "luxluth",
  "bugs": {
    "url": "https://github.com/luxluth/onigiri/issues"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./menu": {
      "types": "./dist/menu/index.d.ts",
      "import": "./dist/menu/index.mjs",
      "require": "./dist/menu/index.js"
    }
  },
  "version": "0.2.14",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "terser": "^5.16.4",
    "tsup": "^6.6.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "tsup --entry src/index.ts --entry src/menu/index.ts --format cjs,esm --dts",
    "dev": "tsup --entry src/index.ts --entry src/menu/index.ts --watch --format cjs,esm --dts",
    "lint": "tsc",
    "release": "pnpm lint && pnpm build && changeset publish"
  }
}
